{
    "data": {
      "url": "data/cars.json"
    },
    "mark": {
        "type": "bar",
        "clip": true
    },
    "encoding": {
      "y": {
        "field": "Cylinders",
        "type": "ordinal"
      },
      "x": {
        "field": "Horsepower",
        "type": "quantitative",
        "aggregate": "distinct",
        "scale": {
            "domain": [20, 60],
            "nice": true
        }
      }
    }
  }